class Animal is
    var name : String()

    this() is end

    method getName() is
        return name
    end
end

class Dog extends Animal is
    var breed : String()
    
    this() is end

    method getBreed() is
        return breed
    end
end

class Main is
    this() is
        var d : Dog()
        d.name := String("Buddy")
        d.breed := String("Golden Retriever")

        var res1 : String()
        var res2 : String()

        res1 := d.getName()
        res2 := d.getBreed()

        res1.print()
        res2.print()
    end
end
