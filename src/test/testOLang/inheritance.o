class Animal is
    var name : String()

    this() is end

    method getName() is
        return name
    end
end

class Dog extends Animal is
    var breed : String()
    
    this() is end

    method getBreed() is
        return breed
    end
end

class Main is
    this() is
        var d : Dog()
        d.name := "Buddy"
        d.breed := "Golden Retriever"

        var res1 : String()
        var res2 : String()

        res1 := getName()
        res2 := getBreed()

        res1.print()
        res2.print()
    end
end
