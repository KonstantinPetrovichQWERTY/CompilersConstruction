class Calculator is
    this() is end
    method add(a : Integer, b : Integer) : Integer is
        return a.Plus(b)
    end
end

class Main is
    this() is
        var calc : Calculator(1).plus(1)
        Calculator(1).plus(1)
        var result : Integer(0)
        result := calc.add(5, 3)
        result.print()        
    end
end
