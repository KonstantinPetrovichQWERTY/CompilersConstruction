class Point is
    var x : Integer(0)
    var y : Integer(1)

    this() is end

    method getX() : Integer is
        return x
    end

    method getY() : Integer is
        return y
    end

    method setX(newValue : Integer) is
        x := newValue
    end

    method setY(newValue : Integer) is
        y := newValue
    end
end

class Main is
    this() is
        var p : Point()
        p.setX(Integer(2))
        p.setY(Integer(4))
        p.getX.print()
        p.getY.print()
    end
end
