class Animal is
    this() is end

    method makeSound() is
        print(String("Animal makes a sound"))
    end
end

class Dog extends Animal is
    this() is end

    method makeSound() is
        print(String("Dog barks"))
    end
end

class Cat extends Animal is
    this() is end

    method makeSound() is
        print(String("Cat meows"))
    end
end

class Main is
    this() is
        var animal : Animal()
        var dog : Dog()
        var cat : Cat()

        animal.makeSound()
        dog.makeSound()
        cat.makeSound()
    end
end
